# Frontend Environment Variables Example
# Copy this file to .env and update with your actual values

# API Configuration
VITE_API_URL=http://localhost:8080
VITE_API_TIMEOUT=30000

# App Configuration
VITE_APP_NAME="OMI - Plan de Salud Infantil"
VITE_APP_VERSION="1.0.0"
VITE_ENABLE_MOCK=false

# Authentication
VITE_JWT_REFRESH_THRESHOLD=300000  # 5 minutes before expiry

# File Upload
VITE_MAX_FILE_SIZE=10485760  # 10MB in bytes
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Analytics (Google Analytics, etc.)
VITE_GA_TRACKING_ID=

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_LOGS=false

# Clinic Configuration (for branding)
VITE_CLINIC_LOGO_URL=
VITE_CLINIC_PRIMARY_COLOR="#4F46E5"
VITE_CLINIC_SECONDARY_COLOR="#06B6D4"

# External Services
VITE_STRIPE_PUBLISHABLE_KEY=
VITE_FIREBASE_CONFIG=

# Development
VITE_ENABLE_DEVTOOLS=true
VITE_LOG_LEVEL=info
